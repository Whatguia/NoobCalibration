    "configurations": [
        {
            "name":"get_pixel(gdp)启动",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/bin/get_pixel",
            "args": [
                "./data/image.jpg",
                "./data/calibration.json"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
